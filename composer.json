{
  "name": "bokuno/t3-ez-logger",
  "license": "GPL-2.0-or-later",
  "description": "Sometimes you just need a little workaround to debug your code",
  "keywords": [
    "typo3",
    "Logger"
  ],
  "homepage": "https://github.com/BokuNoMaxi/T3-EZ-Logger",
  "authors": [
    {
      "name": "Markus Ketterer",
      "email": "4tmestorage@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "^13.4"
  },
  "require-dev": {
    "ssch/typo3-rector": "^3.1",
    "typo3/testing-framework": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "BokuNo\\T3EZLogger\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "t3ezlogger"
    }
  }
}
  